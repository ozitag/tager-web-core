{
  "name": "@tager/web-core",
  "version": "0.3.7",
  "author": "Ivan Saranchonkau",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/web-core.umd.production.js",
  "module": "dist/web-core.esm.js",
  "typings": "dist/main.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "cross-env NODE_ENV=production tsdx build --format=cjs,esm --entry ./src/main.ts",
    "test": "tsdx test",
    "lint": "tsdx lint --fix --report-file report.json",
    "prebuild": "rimraf dist",
    "prepare": "yarn build"
  },
  "dependencies": {
    "cookie": "^0.4.1",
    "js-cookie": "^3.0.0-rc.1",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.15",
    "nprogress": "^0.2.0",
    "tua-body-scroll-lock": "^1.2.0",
    "typed-contracts": "^2.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.1.0",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.163",
    "@types/node": "12.x",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "babel-jest": "^26.6.0",
    "cross-env": "^7.0.2",
    "husky": "^4.3.0",
    "next": "^10.0.0",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "next": "^10.0.0",
    "react": "^17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint --fix",
      "post-commit": "git update-index --again"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "resolutions": {
    "prettier": "^2.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
